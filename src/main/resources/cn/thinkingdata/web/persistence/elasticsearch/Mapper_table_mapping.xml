<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.thinkingdata.web.persistence.elasticsearch.Mapper_table_mapping">
	<select id="findTableMappingByTableName" resultType="cn.thinkingdata.web.domain.elasticsearch.Do_table_mapping">
